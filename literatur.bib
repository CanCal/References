  author = {{Silva-Rocha}, Rafael and {Mart{\'i}nez-Garc{\'i}a}, Esteban and Calles, Bel{\'e}n and Chavarr{\'i}a, Max and {Arce-Rodr{\'i}guez}, Alejandro and {de las Heras}, Aitor and {P{\'a}ez-Espino}, A. David and {Durante-Rodr{\'i}guez}, Gonzalo and Kim, Juhyun and Nikel, Pablo I. and Platero, Ra{\'u}l and {de Lorenzo}, V{\'i}ctor},
