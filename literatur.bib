@article{gregory2022RecentAdvances,
  title = {Recent Advances toward the Bioconversion of Methane and Methanol in Synthetic Methylotrophs},
  author = {Gregory, Gwendolyn J. and Bennett, R. Kyle and Papoutsakis, Eleftherios T.},
  year = 2022,
