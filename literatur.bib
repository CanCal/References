@article{witthoff2015MetabolicEngineering,
  title = {Metabolic {{Engineering}} of {{Corynebacterium}} Glutamicum for {{Methanol Metabolism}}},
  author = {Witthoff, Sabrina and Schmitz, Katja and Niedenf{\"u}hr, Sebastian and N{\"o}h, Katharina and Noack, Stephan and Bott, Michael and Marienhagen, Jan},
  editor = {Spormann, A. M.},
  year = 2015,
  month = mar,
  journal = {Applied and Environmental Microbiology},
  volume = {81},
