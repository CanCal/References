@article{bennett2018ExpressionHeterologous,
  title = {Expression of Heterologous Non-Oxidative Pentose Phosphate Pathway from {{{\emph{Bacillus}}}}{\emph{ Methanolicus}} and Phosphoglucose Isomerase Deletion Improves Methanol Assimilation and Metabolite Production by a Synthetic {{{\emph{Escherichia}}}}{\emph{ Coli}} Methylotroph},
