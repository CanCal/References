  keywords = {notion},
  file = {C:\Users\canoscaliskan\Zotero\storage\NLGYYAEI\Jakobsen et al. - 2006 - Upregulated Transcription of Plasmid and Chromosomal Ribulose Monophosphate Pathway Genes Is Critica.pdf}
}

