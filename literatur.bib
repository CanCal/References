@article{yurimoto2005AssimilationDissimilation,
  title = {Assimilation, Dissimilation, and Detoxification of Formaldehyde, a Central Metabolic Intermediate of Methylotrophic Metabolism},
  author = {Yurimoto, Hiroya and Kato, Nobuo and Sakai, Yasuyoshi},
  year = 2005,
