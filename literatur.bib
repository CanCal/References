}

@article{zelcbuch2013SpanningHighdimensional,
  title = {Spanning High-Dimensional Expression Space Using Ribosome-Binding Site Combinatorics},
  author = {Zelcbuch, Lior and Antonovsky, Niv and {Bar-Even}, Arren and {Levin-Karp}, Ayelet and Barenholz, Uri and Dayagi, Michal and Liebermeister, Wolfram and Flamholz, Avi and Noor, Elad and Amram, Shira and Brandis, Alexander and Bareia, Tasneem and Yofe, Ido and Jubran, Halim and Milo, Ron},
